{
    "name": "ekinhbayar/slack-driver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ekinhbayar",
            "email": "me@ekins.space"
        }
    ],
    "require": {
        "php": "^7.4",
        "amphp/amp": "^2.2.0",
        "amphp/http-client": "dev-master as 4.0.0",
        "amphp/log": "^1.0",
        "async-bot/core": "dev-master",
        "daverandom/exceptional-json": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.13.4",
        "maglnet/composer-require-checker": "^2.0",
        "phpunit/phpunit": "^8.3",
        "slevomat/coding-standard": "^5.0.4",
        "squizlabs/php_codesniffer": "^3.4.2"
    },
    "autoload": {
        "psr-4": {
            "ekinhbayar\\Driver\\Slack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ekinhbayar\\Driver\\SlackTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
